<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
       
    <xs:element name="radarFile">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="alternateIdentifiers" minOccurs="0"/>
                <xs:element ref="relatedIdentifiers" minOccurs="0"/>
                
                <xs:element ref="creators" minOccurs="0"/>
                <xs:element ref="contributors" minOccurs="0"/>
                
                <xs:element ref="title" minOccurs="1"/>
                <xs:element ref="additionalTitles" minOccurs="0"/>
                <xs:element ref="descriptions" minOccurs="0"/>
                <xs:element ref="keywords" minOccurs="0"/>
                
                <xs:element ref="productionYear" minOccurs="0"/>
                <xs:element ref="language" minOccurs="1"/>
                <xs:element ref="subjectAreas" minOccurs="0"/>
                
                <xs:element ref="resource" minOccurs="0"/>
                <xs:element ref="geoLocations" minOccurs="0"/>
                <xs:element ref="dataSources" minOccurs="0"/>
                <xs:element ref="software" minOccurs="0"/>
                <xs:element ref="processing" minOccurs="0"/>
                
                <xs:element ref="relatedInformations" minOccurs="0"/>
                <xs:element ref="fundingReferences" minOccurs="0"/>
                
            </xs:sequence>
        </xs:complexType>
    </xs:element> 
    
    <xs:element name="alternateIdentifiers">
        <xs:annotation>
            <xs:documentation>An identifier or identifiers other than the primary Identifier applied
                to the resource being registered.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="alternateIdentifier" minOccurs="1" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="alternateIdentifier">
        <!-- dct:bibliographicCitation should be considered for substitutionGroup. -->
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute type="xs:string" name="alternateIdentifierType" use="required" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="relatedIdentifiers">
        <xs:annotation>
            <xs:documentation>Identifiers of related resources. These must be globally unique identifiers.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="relatedIdentifier" minOccurs="1" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="relatedIdentifier">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute type="relatedIdentifierType" name="relatedIdentifierType" use="required" />
                    <xs:attribute name="schemeURI" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>The URI of the affiliation identifier scheme. Example: https://ror.org/</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="relationType" name="relationType" use="required" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <!-- define possible values for related identifier type type element -->
    <xs:simpleType name="relatedIdentifierType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ARK"/>
            <xs:enumeration value="arXiv"/>
            <xs:enumeration value="DOI"/>
            <xs:enumeration value="Handle"/>
            <xs:enumeration value="ISBN"/>
            <xs:enumeration value="ISSN"/>
            <xs:enumeration value="URL"/>
            <xs:enumeration value="URN"/>
            <xs:enumeration value="w3id"/>
            <xs:enumeration value="GND"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- define possible values for relation type type element -->
    <xs:simpleType name="relationType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="IsEditionOf"/>
            <xs:enumeration value="IsCopyOf"/>
            <xs:enumeration value="IsRecensionOf"/>
            <xs:enumeration value="IsOriginalFormOf"/>
            <xs:enumeration value="IsDerivedFrom"/>
            <xs:enumeration value="IsSourceOf"/>
            <xs:enumeration value="IsContinuedBy"/>
            <xs:enumeration value="Continues"/>
            <xs:enumeration value="HasMetadata"/>
            <xs:enumeration value="IsMetadataFor"/>
            <xs:enumeration value="IsPartOf"/>
            <xs:enumeration value="HasPart"/>
            <xs:enumeration value="IsPublishedIn"/>
            <xs:enumeration value="IsCollectedBy"/>
            <xs:enumeration value="Collects"/>
            <xs:enumeration value="HasTranslation"/>
            <xs:enumeration value="IsTranslationOf"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:element name="creators">
        <xs:annotation>
            <xs:documentation>The main researchers involved in producing the data, or the authors of the publication, in priority order.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="creator" minOccurs="1" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="creator">
        <xs:complexType>
            <xs:sequence>
                <xs:element type="notNull" name="creatorName">
                    <xs:annotation>
                        <xs:documentation>Format: forename, optional linking element, surname</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element type="notNull" name="foreName" minOccurs="0" maxOccurs="1" />
                <xs:element type="notNull" name="nameLink" minOccurs="0" maxOccurs="2" />
                <xs:element type="notNull" name="surName" minOccurs="0" maxOccurs="2" />
                <xs:element ref="nameIdentifier" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="creatorAffiliation" minOccurs="0" maxOccurs="1" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:simpleType name="notNull">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    
        <xs:element name="nameIdentifier">
        <xs:annotation>
            <xs:documentation>Uniquely identifies an individual or legal entity.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="notNull">
                    <xs:attribute name="schemeURI" use="optional" type="notNull">
                        <xs:annotation>
                            <xs:documentation>The URI of the name identifier scheme. Example: http://www.orcid.org</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="nameIdentifierScheme" use="required" type="nameIdentifierScheme">
                        <xs:annotation>
                            <xs:documentation>Examples: GND, ORCID, ROR</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:simpleType name="nameIdentifierScheme">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Other"/>
            <xs:enumeration value="ORCID"/>
            <xs:enumeration value="ROR"/>
            <xs:enumeration value="GND"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:element name="creatorAffiliation">
        <xs:annotation>
            <xs:documentation>The organizational or institutional affiliation of the creator.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="notNull">
                    <xs:attribute name="schemeURI" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>The URI of the affiliation identifier scheme. Example: https://ror.org/</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="affiliationIdentifierScheme" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>Example: ROR</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="affiliationIdentifier" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>Uniquely identifies the organizational affiliation of the creator. Example: https://ror.org/04aj4c181</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="contributors">
        <xs:annotation>
            <xs:documentation>The institution or person responsible for collecting, managing, distributing, or otherwise contributing to the development or creation of the resource.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="contributor" minOccurs="1" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="contributor">
        <xs:complexType>
            <xs:sequence>
                <xs:element type="notNull" name="contributorName">
                    <xs:annotation>
                        <xs:documentation>Format: forename, optional linking element, surname</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element type="notNull" name="foreName" minOccurs="0" maxOccurs="1" />
                <xs:element type="notNull" name="linkName" minOccurs="0" maxOccurs="2" />
                <xs:element type="notNull" name="surName" minOccurs="0" maxOccurs="2" />
                <xs:element ref="nameIdentifier" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="contributorAffiliation" minOccurs="0" maxOccurs="1" />
            </xs:sequence>
            <xs:attribute type="contributorType" name="contributorType" use="required" />
        </xs:complexType>
    </xs:element>
    
    <!-- define possible values for contributor type element -->
    <xs:simpleType name="contributorType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ContactPerson"/>
            <xs:enumeration value="DataCollector"/>
            <xs:enumeration value="Editor"/>
            <xs:enumeration value="HostingInstitution"/>
            <xs:enumeration value="Producer"/>
            <xs:enumeration value="ProjectLeader"/>
            <xs:enumeration value="ProjectManager"/>
            <xs:enumeration value="ProjectMember"/>
            <xs:enumeration value="RegistrationAgency"/>
            <xs:enumeration value="RelatedPerson"/>
            <xs:enumeration value="Researcher"/>
            <xs:enumeration value="ResearchGroup"/>
            <xs:enumeration value="Sponsor"/>
            <xs:enumeration value="Supervisor"/>
            <xs:enumeration value="Translator"/>
            <xs:enumeration value="Scribe"/>
            <xs:enumeration value="Other"/>
        </xs:restriction>
    </xs:simpleType>
    
    
    <xs:element name="contributorAffiliation">
        <xs:annotation>
            <xs:documentation>The organizational or institutional affiliation of the contributor.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="notNull">
                    <xs:attribute name="schemeURI" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>The URI of the affiliation identifier scheme. Example: https://ror.org/</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="affiliationIdentifierScheme" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>Example: ROR</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="affiliationIdentifier" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>Uniquely identifies the organizational affiliation of the contributor. Example: https://ror.org/04aj4c181</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="title" type="notNull">
        <xs:annotation>
            <xs:documentation>A heading or caption by which a resource is well described.</xs:documentation>
        </xs:annotation>
    </xs:element>
    
    <xs:element name="additionalTitles">
        <xs:annotation>
            <xs:documentation>Complementary textual information to the main title of the resource.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="additionalTitle" minOccurs="1" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="additionalTitle">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute type="additionalTitleType" name="additionalTitleType" use="required" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <!-- define possible values for additional title type element -->
    <xs:simpleType name="additionalTitleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Subtitle"/>
            <xs:enumeration value="TranslatedTitle"/>
            <xs:enumeration value="AlternativeTitle"/>
            <xs:enumeration value="Other"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:element name="descriptions">
        <xs:annotation>
            <xs:documentation>A textual description containing additional information about resource (English is strongly recommended as primary language).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="description" minOccurs="1" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="description">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute type="descriptionType" name="descriptionType" use="required">
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    
    <!-- define possible values for description type type element -->
    <xs:simpleType name="descriptionType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Abstract"/>
            <xs:enumeration value="Object"/>
            <xs:enumeration value="Method"/>
            <xs:enumeration value="TableOfContents"/>
            <xs:enumeration value="TechnicalInfo"/>
            <xs:enumeration value="TechnicalRemarks"/>
            <xs:enumeration value="Other"/>
        </xs:restriction>
    </xs:simpleType>
    
    
    <xs:element name="keywords">
        <xs:annotation>
            <xs:documentation>Keyword(s) describing the subject focus of the resource (English is
                strongly recommended as primary language).
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="keyword" minOccurs="1" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="keyword">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="notNull">
                    <xs:attribute name="keywordScheme" type="keywordScheme" use="optional">
                        <xs:annotation>
                            <xs:documentation>The name of the keyword scheme. Example: GND.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="schemeURI" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>The URI of the keyword scheme. Example: https://d-nb.info/gnd/</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="valueURI" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>The URI of the keyword term.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="classificationCode" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>The classification code or identifier used for the keyword term.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ontologyURI" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>The URI of the keyword ontology. Example: https://terminology.tib.eu/ts/ontologies/afo</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ontologyId" type="notNull" use="optional">
                        <xs:annotation>
                            <xs:documentation>The identifier of the keyword ontology. Example: afo</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:simpleType name="keywordScheme">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Other"/>
            <xs:enumeration value="GND"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:element name="productionYear" type="yearOrUnknown">
        <xs:annotation>
            <xs:documentation>Year in which the resource was created or the resource refers to. (YYYY or YYYY-YYYY or "unknown").</xs:documentation>
        </xs:annotation>
    </xs:element>
    
    <xs:simpleType name="yearOrUnknown">
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{4}|\d{4}-\d{4}|unknown"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:element name="language" type="language">
        <xs:annotation>
            <xs:documentation>Main language used or relevant to the resource.</xs:documentation>
        </xs:annotation>
    </xs:element>
    
    <xs:simpleType name="language">
        <xs:annotation>
            <xs:documentation>Select three letter codes from ISO 639-3.</xs:documentation>
        </xs:annotation>
        
        <xs:restriction base="xs:string">   
            <xs:enumeration value="ara"><xs:annotation><xs:documentation><title xml:lang="en">Arabic</title><title xml:lang="de">Arabisch</title></xs:documentation></xs:annotation></xs:enumeration>
